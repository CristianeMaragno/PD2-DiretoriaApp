<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Vermelho"
    tools:context=".MenuActivity">

    <ImageView
        android:id="@+id/calendarioIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/recadosIcon"
        app:layout_constraintTop_toTopOf="@+id/recadosIcon"
        app:srcCompat="@drawable/icon_calendario" />

    <ImageView
        android:id="@+id/usersIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toEndOf="@+id/calendarioIcon"
        app:layout_constraintTop_toTopOf="@+id/calendarioIcon"
        app:srcCompat="@drawable/icon_users" />

    <ImageView
        android:id="@+id/saidaIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonLogout"
        app:layout_constraintStart_toStartOf="@+id/recadosIcon"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/icon_saida" />

    <ImageView
        android:id="@+id/financeiroIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintStart_toStartOf="@+id/usersIcon"
        app:layout_constraintTop_toTopOf="@+id/diarioIcon"
        app:srcCompat="@drawable/icon_financeiro" />

    <ImageView
        android:id="@+id/recadosIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintBottom_toTopOf="@+id/buttonLogout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/identificadorDiretoria"
        app:layout_constraintVertical_bias="0.16000003"
        app:srcCompat="@drawable/icon_recados" />

    <ImageView
        android:id="@+id/diarioIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintStart_toStartOf="@+id/calendarioIcon"
        app:layout_constraintTop_toTopOf="@+id/saidaIcon"
        app:srcCompat="@drawable/icon_professor" />

    <ImageView
        android:id="@+id/ouvidoriaIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/usersIcon"
        app:layout_constraintTop_toTopOf="@+id/usersIcon"
        app:srcCompat="@drawable/icon_chat" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"
        android:backgroundTint="@color/Amarelo"
        android:text="LOG OUT"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/identificadorDiretoria" />

    <TextView
        android:id="@+id/identificadorDiretoria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="25dp"
        android:paddingRight="8dp"
        android:paddingBottom="8dp"
        android:text="Membro da Direção"
        android:textColor="@color/Amarelo"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="Usuários"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/usersIcon"
        app:layout_constraintStart_toStartOf="@+id/usersIcon"
        app:layout_constraintTop_toBottomOf="@+id/usersIcon" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Financeiro"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/financeiroIcon"
        app:layout_constraintStart_toStartOf="@+id/financeiroIcon"
        app:layout_constraintTop_toBottomOf="@+id/financeiroIcon" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendário"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/calendarioIcon"
        app:layout_constraintStart_toStartOf="@+id/calendarioIcon"
        app:layout_constraintTop_toBottomOf="@+id/calendarioIcon" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Saída"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/saidaIcon"
        app:layout_constraintStart_toStartOf="@+id/saidaIcon"
        app:layout_constraintTop_toBottomOf="@+id/saidaIcon" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recados"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/recadosIcon"
        app:layout_constraintStart_toStartOf="@+id/recadosIcon"
        app:layout_constraintTop_toBottomOf="@+id/recadosIcon" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ouvidoria"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/ouvidoriaIcon"
        app:layout_constraintStart_toStartOf="@+id/ouvidoriaIcon"
        app:layout_constraintTop_toBottomOf="@+id/ouvidoriaIcon" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diário"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/diarioIcon"
        app:layout_constraintStart_toStartOf="@+id/diarioIcon"
        app:layout_constraintTop_toBottomOf="@+id/diarioIcon" />


</androidx.constraintlayout.widget.ConstraintLayout>